{
    outpipe
    {
        type        zeroGradient;
    }
    top
    {
        type        zeroGradient;
    }
    left_forward
    {
        type        zeroGradient;
    }
    right_forward
    {
        type        zeroGradient;
    }
    left_back
    {
        type        zeroGradient;
    }
    right_back
    {
        type        zeroGradient;
    }
    bottom
    {
        type        zeroGradient;
    }
    inlet
    {
        type        zeroGradient;
    }
    inpipe
    {
        type        zeroGradient;
    }
    outlet
    {
        type        fixedValue;
        value        uniform 0;
    }
}
